public class TetrisS2 extends TetrisPiece {
	public TetrisS2() {
		boolean[][][] filled = {
				{ { false, true, false, false }, { true, true, false, false },
						{ true, false, false, false },
						{ false, false, false, false } },

				{ { true, true, false, false }, { false, true, true, false },
						{ false, false, false, false },
						{ false, false, false, false } },

				{ { false, true, false, false }, { true, true, false, false },
						{ true, false, false, false },
						{ false, false, false, false } },

				{ { true, true, false, false }, { false, true, true, false },
						{ false, false, false, false },
						{ false, false, false, false } } };
		this.filledSquares = filled;
	}
}
