public class TetrisSquare extends TetrisPiece {
	public TetrisSquare() {
		boolean[][][] filled = {
				{ { true, true, false, false }, { true, true, false, false },
						{ false, false, false, false },
						{ false, false, false, false } },

				{ { true, true, false, false }, { true, true, false, false },
						{ false, false, false, false },
						{ false, false, false, false }, },

				{ { true, true, false, false }, { true, true, false, false },
						{ false, false, false, false },
						{ false, false, false, false }, },

				{ { true, true, false, false }, { true, true, false, false },
						{ false, false, false, false },
						{ false, false, false, false }, } };
		this.filledSquares = filled;
	}
}
